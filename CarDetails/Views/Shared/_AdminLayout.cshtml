
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from radixtouch.in/templates/snkthemes/grexsan/source/light/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 12 Oct 2020 13:41:29 GMT -->
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Grexsan - Admin Dashboard Template</title>
    <!-- General CSS Files -->
    <link rel="stylesheet" href="~/admintemplate/css/app.min.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/admintemplate/css/style.css">
    <link rel="stylesheet" href="~/admintemplate/css/components.css">
    <link rel="stylesheet" href="~/admintemplate/bundles/bootstrap-social/bootstrap-social.css">
    <link rel="stylesheet" href="~/admintemplate/bundles/flag-icon-css/css/flag-icon.min.css">
    <!-- Custom style CSS -->

    <link rel='shortcut icon' type='image/x-icon' href='assets/img/favicon.ico' />

</head>

<body>
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg collapse-btn">
                                <i class="fas fa-bars"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                                <i class="fas fa-expand"></i>
                            </a>
                        </li>
                        <li>
                            <div class="search-group">
                                <span class="nav-link nav-link-lg" id="search">
                                    <i class="fa fa-search" aria-hidden="true"></i>
                                </span>
                                <input type="text" class="search-control" placeholder="search" aria-label="search" aria-describedby="search">
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="index.html">
                            <img alt="image" src="~/admintemplate/img/logo.png" class="header-logo" />
                            <span class="logo-name">Grexsan</span>
                        </a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="dropdown active" style="display: block;">
                            <div class="sidebar-profile">
                                <div class="siderbar-profile-pic">
                                    <img src="~/admintemplate/img/users/user-6.png" class="profile-img-circle box-center" alt="User Image">
                                </div>
                                <div class="siderbar-profile-details">
                                    <div class="siderbar-profile-name"> Caner AY </div>
                                    <div class="siderbar-profile-position">Manager </div>
                                </div>
                                <div class="sidebar-profile-buttons">
                                    <a class="tooltips waves-effect waves-block toggled" href="profile.html" data-toggle="tooltip" title="" data-original-title="Profil">
                                        <i class="fas fa-user sidebarQuickIcon"></i>
                                    </a>
                                    <a class="tooltips waves-effect waves-block" href="auth-login.html" data-toggle="tooltip" title="" data-original-title="Çıkış">
                                        <i class="fas fa-share-square sidebarQuickIcon"></i>
                                    </a>
                                </div>
                            </div>
                        </li>
                        <li><a class="nav-link" href="/Admin/Index"><i class="far fa-calendar-alt"></i><span>Anasayfa</span></a></li>
                        <li class="dropdown">
                            <a href="#" class="nav-link has-dropdown"><i class="fab fa-atlassian"></i><span>İşlemler</span></a>
                            <ul class="dropdown-menu">
                                <li><a class="nav-link" href="/Admin/Cars">Araçlar</a></li>
                                <li><a class="nav-link" href="/Admin/CaseType">Kasa Tipi</a></li>
                                <li><a class="nav-link" href="/Admin/Brand">Araç Marka</a></li>
                                <li><a class="nav-link" href="/Admin/Comments">Yorumlar</a></li>
                            </ul>
                        </li>
                    </ul>
                </aside>
            </div>
            <div class="main-content" style="min-height: 627px;">
            @RenderBody()
            </div>
            <!-- Main Content -->
            <footer class="main-footer">
                <div class="footer-left">
                    Copyright &copy; 2020 <div class="bullet"></div> Design By <a href="#">Snkthemes</a>
                </div>
                <div class="footer-right">
                </div>
            </footer>
        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="~/admintemplate/js/app.min.js"></script>
    <!-- JS Libraies -->
    <script src="~/admintemplate/bundles/echart/echarts.js"></script>

    <script src="~/admintemplate/bundles/chartjs/chart.min.js"></script>
    <script src="~/admintemplate/bundles/apexcharts/apexcharts.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/admintemplate/js/page/index.js"></script>
    <!-- Template JS File -->
    <script src="~/admintemplate/js/scripts.js"></script>
    <script src="~/admintemplate/bundles/jquery.sparkline.min.js"></script>

</body>


